{
    "manifest_version": 3,
    "name": "Google Maps Trip Cost",
    "version": "0.1.0",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
}